//还款（修改代码）
 function checkCodePs(){
 	$("#repayButton").removeAttr("onclick");
     var payPs = $.trim($("#payPs").val());
     var imageCode = $.trim($("#imageCode").val());
     //交易密码
     if(payPs==null || payPs==""){
     	$("#repayButton").attr("onclick","checkCodePs();");
         $('#Errorps').text("交易密码不能为空");    
         return;
     }else{
         $('#Errorps').text("");    
    }
     //验证码
     if(imageCode==null || imageCode==""){
     	$("#repayButton").attr("onclick","checkCodePs();");
         $('#ErrorCode').text("验证码不能为空");    
         return;
     }else{
         $('#ErrorCode').text("");    
     }
     
    layer.load('还款中...',5);
    $.ajax({
         url:'/account/myLoan/json/Repay.json',
         type:'post',
         async:false,
         data:$('#submitted').serialize(),
         success: function(data){
             if(data.success){
                 layer.alert(data.message, 1, function(){
                     window.location.href="/account/myLoan/myloan?LoanStateType=15";
                 });         
             }else if(data.code==502){//授权异常
             	layer.alert(data.message, 8, function(){
                     window.location.href="/account/myAccount/bindAccount";
                 });
             }else{
             	$("#repayButton").attr("onclick","checkCodePs();");
                 layer.alert(data.message,8);
                 changeCode();
             }
         },
         error : function(XMLHttpRequest, textStatus, errorThrown){                    
               if(XMLHttpRequest.status==403){
                   layer.alert('表单不能重复提交，请刷新页面再还款!', 8, function(){
                       location.reload();    //刷新当前页面
                     });
               }else{
                   alert("通信异常!");
               }
          }
    });    
 }

