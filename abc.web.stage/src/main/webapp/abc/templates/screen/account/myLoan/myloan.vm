 var aheadRepayDiv;
 
 //提前还款
 function aheadRepay(loanId){
     $.ajax({
        url: '/account/myLoan/json/getAheadRepayData.json?loanId='+loanId,
        success: function(data){
            $("#shouldCapital").text(data.shouldCapital);
            $("#shouldInterest").text(data.shouldInterest);
            $("#shouldServeFee").text(data.shouldServeFee);
            $("#shouldAll").text(data.shouldAll);
            $("#aheadDiv #loanId").val(loanId);
            $("#Errorps2").text("");
            var aheadRepayDiv = $.layer
            ({
                type: 1,
                title: "提前还款申请",
                area: ['610px', '400px'],
                border: [6, 0.3, '#000'], //去掉默认边框
                shift: 'top', //从左动画弹出
                page: 
                {
                    dom: '#aheadDiv', 
                    ok: function(datas){/*ajax请求完毕后执行的回调，datas是异步传递过来的值*/}
                }
            });
            //自设关闭
            $('#cancel').on('click', function(){
                    layer.close(aheadRepayDiv);
            }); 
        },
        error: function(){
            alert("系统错误");
        }
     });
     
 }
 
 function aheadRepayApply(){
 	var loanId = $("#aheadDiv #loanId").val();
 	if($("#payState_"+loanId).val()==1){
 		$("#Errorps2").text("当前状态为还款中不能提前还款！");
 		$("#aheadRepayButton").attr("onclick","aheadRepayApply();");
 		return false;
 	}
 	$("#aheadRepayButton").removeAttr("onclick");
    $.ajax({
         url: '/account/myLoan/json/aheadRepayApply.json',
         type: 'POST',
         data: $("#aheadForm").serialize(),
         success: function(data){
             if(data.success==false){
                 $("#Errorps2").text(data.message);
                 $("#aheadRepayButton").attr("onclick","aheadRepayApply();");
             }else {
                 layer.alert("申请成功！",1,function(){
                     location.reload();
                     //layer.close(aheadRepayDiv);
                 });
             }
         }
     });
 }
</script>


                   
