                        function repay(payid,loanPeriod,payCapital,payInterest,payCapIn,loanId,RepayType,payFine,payBreachFine,payServiceFee)
                        {
                            var date = $("#"+payid).text();
                            var payTimestr = date+' 00:00:00';
                            var payTime  = new Date(payTimestr);
                            //本期还款
                            var repaymentTotal=changeTwoDecimal(payCapIn+payFine+payBreachFine+payServiceFee);
                            
                            $("#loanPeriod").text(loanPeriod+'期');
                            $("#payCapital").text(payCapital+'元');
                            $("#payInterest").text(payInterest+'元');
                            $("#payFine").text(payFine+'元');
                            $("#payServiceFee").text(payServiceFee+'元');
                            //新增违约罚金  夏同同 20160410
                            $("#payBreachFine").text(payBreachFine+'元');
                            $("#repay").text(repaymentTotal+'元');
                            $("#payId").val(payid);
                            $("#loanId").val(loanId);
                            $("#payPs").val("");
                            $("#imageCode").val("");
                            $('#Errorps').text("");
                            $('#ErrorCode').text("");
                            $("#xieyi").text("");
                            changeCode();
                            messageReply(); 
                        }
                        
                        //将浮点数转换成两位的小数-四舍五入
                        function changeTwoDecimal(floatvar)
                        {
                            var f_x = parseFloat(floatvar);
                            var f_x = Math.round(floatvar*100)/100;
                            return f_x;
                        }
                        
                        
