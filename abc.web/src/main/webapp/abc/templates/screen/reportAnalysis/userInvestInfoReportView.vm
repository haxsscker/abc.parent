<div id="layout">
    <div region="center" title="注册用户明细表" iconcls="icon-users" class="regionCenter">
    <div class="SearchForm" id="SearchForm">
        <form id="searchForm1">
            <fieldset>
                <legend class="ml10">信息查询</legend>
                 <div class="clearfloat search-wrap">
                        <ul class="fl search-width">
                            <li>
                                <span>真实姓名：</span>
                                <span>
                                    <input type="text" name="name" class="text-input ipt-w80" id="name"/></span>
                            </li>
                              
                            <li>
                                <span>注册时间：</span>
                                <span>
                                    <input type="text" class="text-input easyui-datetimebox w100"  name="startRegisterDate" id="startRegisterDate"/>
                                 	   至
                                    <input type="text" class="text-input easyui-datetimebox w100" name="endRegisterDate"  id="endRegisterDate" />
                                </span>
                            </li>
                              
                        </ul>
                        <div class="fl search-div">
                            <a id='Search' class="btns btns-primary wheight SearchHide" href="javascript:;">搜索</a>
                        </div>
                    </div>
            </fieldset>
        </form>
    </div>
    <div id="toolbar">
       $control.setTemplate("common/showButton.vm")
    </div>
    <table id="reportGrid" toolbar="#toolbar"></table>
</div>
</div>

